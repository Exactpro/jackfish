<?xml version="1.0" encoding="UTF-8"?>

<?import com.exactprosystems.jf.tool.CssVariables?>
<?import com.exactprosystems.jf.tool.custom.controls.field.CustomFieldWithButton?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="513.0" prefWidth="1105.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exactprosystems.jf.tool.dictionary.actions.ActionsController">
	<fx:define>
		<CssVariables fx:id="verMin" fx:constant="VERTICAL_MIN" />
		<CssVariables fx:id="verMid" fx:constant="VERTICAL_MID" />
		<CssVariables fx:id="verMax" fx:constant="VERTICAL_MAX" />

		<CssVariables fx:id="horMin" fx:constant="HORIZONTAL_MIN" />
		<CssVariables fx:id="horMid" fx:constant="HORIZONTAL_MID" />
		<CssVariables fx:id="horMax" fx:constant="HORIZONTAL_MAX" />
	</fx:define>
	<center>
		<GridPane fx:id="mainGrid">
			<styleClass>
				<CssVariables fx:constant="HGAP_MAX" />
				<CssVariables fx:constant="VGAP_MID" />
			</styleClass>
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="370.0" minWidth="370.0" prefWidth="370.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="200.0" prefHeight="200.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<VBox alignment="TOP_CENTER" GridPane.halignment="CENTER">
					<children>
						<GridPane fx:id="elementActionsGrid" hgap="4" maxHeight="220" minHeight="220" prefHeight="220" vgap="4" VBox.vgrow="SOMETIMES">
							<columnConstraints>
								<ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints />
							</columnConstraints>
							<rowConstraints>
								<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
								<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
								<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
								<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
								<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
								<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
								<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
							</rowConstraints>
							<children>
								<HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2">
									<children>
										<ComboBox fx:id="comboBoxApps" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
										<Label />
										<Button fx:id="btnStartApplication" mnemonicParsing="false" onAction="#startApplication" />
										<Label />
										<Button fx:id="btnConnectApplication" mnemonicParsing="false" onAction="#connectApplication" />
										<Label />
										<Button fx:id="btnStop" mnemonicParsing="false" onAction="#stopConnection" />
									</children>
								</HBox>
								<Label text="Stored connection:" GridPane.halignment="LEFT" GridPane.rowIndex="1" />
								<ComboBox fx:id="comboBoxAppsStore" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
								<HBox fx:id="hBoxDoIt" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="3">
									<children>
										<Label />
									</children>
								</HBox>
								<HBox alignment="CENTER" GridPane.columnSpan="2" GridPane.rowIndex="2">
									<children>
										<CustomFieldWithButton fx:id="tfSendKeys" onAction="#sendKeysAction" GridPane.columnSpan="2" GridPane.rowIndex="2" HBox.hgrow="ALWAYS" />
										<Label />
									</children>
								</HBox>
								<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sendKeysAction" text="Send keys" GridPane.columnIndex="2" GridPane.rowIndex="2" />
								<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#doIt" text="Do it" GridPane.columnIndex="2" GridPane.rowIndex="3" />
								<HBox alignment="CENTER" GridPane.rowIndex="4">
									<children>
										<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#clickAction" text="Click" GridPane.rowIndex="4" HBox.hgrow="ALWAYS" />
										<Label />
									</children>
								</HBox>
								<HBox alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="4">
									<children>
										<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#findAction" text="Find" GridPane.columnIndex="1" GridPane.rowIndex="4" HBox.hgrow="ALWAYS" />
										<Label />
									</children>
								</HBox>
								<Button maxWidth="114.0" minWidth="114.0" mnemonicParsing="false" onAction="#refresh" text="Refresh" GridPane.columnIndex="2" GridPane.rowIndex="4" />
								<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="3" GridPane.rowIndex="5">
									<children>
										<Label text="Switch to:" />
										<Label minWidth="4.0" />
										<ComboBox fx:id="comboBoxWindows" maxWidth="1.7976931348623157E308" onAction="#changeWindow" prefWidth="150.0" HBox.hgrow="ALWAYS" />
									</children>
								</HBox>
								<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="3" GridPane.rowIndex="6">
									<children>
										<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#switchToCurrent" text="Switch to current" HBox.hgrow="ALWAYS" />
										<Label maxWidth="4.0" minWidth="4.0" />
										<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#switchToParent" text="Switch to parent" HBox.hgrow="ALWAYS" />
									</children>
								</HBox>
								<Label maxWidth="1.7976931348623157E308" text="Application plugin:" />
							</children>
						</GridPane>
					</children>
				</VBox>
				<VBox alignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER">
					<children>
						<ScrollPane fitToHeight="true" fitToWidth="true" prefViewportHeight="397.0" prefViewportWidth="1103.0" HBox.hgrow="ALWAYS">
							<content>
								<Label fx:id="imageArea" contentDisplay="GRAPHIC_ONLY" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
							</content>
						</ScrollPane>
					</children>
				</VBox>
			</children>
		</GridPane>
	</center>
</BorderPane>
