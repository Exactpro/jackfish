<?import com.exactprosystems.jf.tool.CssVariables?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox prefHeight="87.0" prefWidth="1453.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	  fx:controller="com.exactprosystems.jf.tool.dictionary.navigation.NavigationController">
	<children>
		<VBox prefHeight="200.0" prefWidth="100.0" fx:id="vBoxWindow">
			<children>
				<Label>
					<styleClass>
						<CssVariables fx:constant="VERTICAL8"/>
					</styleClass>
				</Label>
				<HBox alignment="CENTER_LEFT">
					<children>
						<Button fx:id="btnNewDialog" alignment="CENTER" mnemonicParsing="false" onAction="#newWindow">
							<tooltip>
								<Tooltip text="Create new window"/>
							</tooltip>
						</Button>
						<Label>
							<styleClass>
								<CssVariables fx:constant="HORIZONTAL4"/>
							</styleClass>
						</Label>
						<Button fx:id="btnDeleteDialog" alignment="CENTER" mnemonicParsing="false" onAction="#deleteWindow">
							<tooltip>
								<Tooltip text="Delete selected window"/>
							</tooltip>
						</Button>
						<Label>
							<styleClass>
								<CssVariables fx:constant="HORIZONTAL4"/>
							</styleClass>
						</Label>
						<Button fx:id="btnCopyDialog" mnemonicParsing="false" onAction="#copyDialog">
							<tooltip>
								<Tooltip text="Copy current dialog with all elements"/>
							</tooltip>
						</Button>
						<Label>
							<styleClass>
								<CssVariables fx:constant="HORIZONTAL4"/>
							</styleClass>
						</Label>
						<Button fx:id="btnPasteDialog" mnemonicParsing="false" onAction="#pasteDialog">
							<tooltip>
								<Tooltip text="Paste copied dialog"/>
							</tooltip>
						</Button>
					</children>
				</HBox>
			</children>
		</VBox>
		<Label>
			<styleClass>
				<CssVariables fx:constant="VERTICAL16"/>
			</styleClass>
		</Label>
		<VBox alignment="CENTER_LEFT" maxWidth="100" minWidth="100" prefWidth="100" GridPane.columnIndex="1" GridPane.valignment="CENTER">
			<children>
				<RadioButton id="Self" mnemonicParsing="false" onAction="#changeSection" text="Self">
					<toggleGroup>
						<ToggleGroup fx:id="groupSection"/>
					</toggleGroup>
				</RadioButton>
				<Label>
					<styleClass>
						<CssVariables fx:constant="VERTICAL4"/>
					</styleClass>
				</Label>
				<RadioButton id="OnOpen" mnemonicParsing="false" onAction="#changeSection" text="On open" toggleGroup="$groupSection"/>
				<Label>
					<styleClass>
						<CssVariables fx:constant="VERTICAL4"/>
					</styleClass>
				</Label>
				<RadioButton id="Run" mnemonicParsing="false" onAction="#changeSection" text="Run" toggleGroup="$groupSection"/>
				<Label>
					<styleClass>
						<CssVariables fx:constant="VERTICAL4"/>
					</styleClass>
				</Label>
				<RadioButton id="OnClose" mnemonicParsing="false" onAction="#changeSection" text="On close" toggleGroup="$groupSection"/>
				<Label>
					<styleClass>
						<CssVariables fx:constant="VERTICAL4"/>
					</styleClass>
				</Label>
				<RadioButton id="Close" mnemonicParsing="false" onAction="#changeSection" text="Close" toggleGroup="$groupSection"/>
			</children>
		</VBox>
		<Label>
			<styleClass>
				<CssVariables fx:constant="VERTICAL16"/>
			</styleClass>
		</Label>
		<VBox prefHeight="200.0" prefWidth="100.0" fx:id="vBoxElement" GridPane.columnIndex="2">
			<children>
				<Label>
					<styleClass>
						<CssVariables fx:constant="VERTICAL8"/>
					</styleClass>
				</Label>
				<HBox fx:id="hBoxElement">
					<children>
						<Button fx:id="btnNewElement" mnemonicParsing="false" onAction="#newElement">
							<tooltip>
								<Tooltip text="Create new Element"/>
							</tooltip>
						</Button>
						<Label>
							<styleClass>
								<CssVariables fx:constant="HORIZONTAL4"/>
							</styleClass>
						</Label>
						<Button fx:id="btnDeleteElement" mnemonicParsing="false" onAction="#deleteElement">
							<tooltip>
								<Tooltip text="Delete current element"/>
							</tooltip>
						</Button>
						<Label>
							<styleClass>
								<CssVariables fx:constant="HORIZONTAL4"/>
							</styleClass>
						</Label>
						<Button fx:id="btnCopyElement" mnemonicParsing="false" onAction="#copyElement">
							<tooltip>
								<Tooltip text="Copy current element"/>
							</tooltip>
						</Button>
						<Label>
							<styleClass>
								<CssVariables fx:constant="HORIZONTAL4"/>
							</styleClass>
						</Label>
						<Button fx:id="btnPasteElement" mnemonicParsing="false" onAction="#pasteElement">
							<tooltip>
								<Tooltip text="Paste copied element"/>
							</tooltip>
						</Button>
						<Label>
							<styleClass>
								<CssVariables fx:constant="HORIZONTAL4"/>
							</styleClass>
						</Label>
						<Button fx:id="btnTestWindow" mnemonicParsing="false" onAction="#testingDialog">
							<tooltip>
								<Tooltip text="Test all controls"/>
							</tooltip>
						</Button>
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</HBox>
