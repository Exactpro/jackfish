<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exactprosystems.jf.tool.git.branch.GitBranchController">
	<fx:define>
		<CssVariables fx:id="verMin" fx:constant="VERTICAL_MIN"/>
		<CssVariables fx:id="verMid" fx:constant="VERTICAL_MID"/>
		<CssVariables fx:id="verMax" fx:constant="VERTICAL_MAX"/>

		<CssVariables fx:id="horMin" fx:constant="HORIZONTAL_MIN"/>
		<CssVariables fx:id="horMid" fx:constant="HORIZONTAL_MID"/>
		<CssVariables fx:id="horMax" fx:constant="HORIZONTAL_MAX"/>
	</fx:define>
	<children>
		<Label id="$verMid"/>
		<HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
			<children>
				<Label id="$horMid" />
				<BorderPane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
					<right>
						<HBox prefHeight="330.0" prefWidth="115.0">
							<children>
								<Label id="$horMid" />
								<VBox fx:id="vBox" alignment="TOP_CENTER" prefHeight="370.0" prefWidth="178.0" BorderPane.alignment="CENTER" HBox.hgrow="ALWAYS">
									<children>
										<Button fx:id="btnNewBranch" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#newBranch" text="New" VBox.vgrow="ALWAYS" />
										<Label id="$verMid" />
										<Button fx:id="btnRenameBranch" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#renameBranch" text="Rename" />
										<Label id="$verMid" />
										<Button fx:id="btnCheckoutBranch" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#checkoutBranch" text="Checkout" />
										<Label id="$verMid" />
										<Button fx:id="btnMergeBranch" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#mergeBranch" text="Merge" />
										<Label id="$verMax" />
										<Button fx:id="btnDeleteBranch" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#deleteBranch" text="Delete" />
									</children>
								</VBox>
							</children>
						</HBox>
					</right>
					<center>
						<TreeView fx:id="treeView" prefHeight="370.0" prefWidth="259.0" BorderPane.alignment="CENTER" />
					</center>
				</BorderPane>
				<Label id="$horMid" />
			</children>
		</HBox>
		<Label id="$verMid" />
		<HBox alignment="CENTER_RIGHT" prefHeight="25.0" prefWidth="200.0">
			<children>
				<Button fx:id="btnClose" mnemonicParsing="false" text="Close" onAction="#hide" />
				<Label id="$horMid" />
			</children>
		</HBox>
	</children>
</VBox>
