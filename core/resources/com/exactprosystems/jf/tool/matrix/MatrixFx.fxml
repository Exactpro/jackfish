												<Button fx:id="btnStopDefaultClient" mnemonicParsing="false" onAction="#stopDefaultClient"/>
											</children>
											<padding>
												<Insets left="8.0"/>
											</padding>
										</HBox>
									</right>
								</BorderPane>
								<Separator orientation="VERTICAL" prefHeight="35.0" prefWidth="0.0" GridPane.columnIndex="1" GridPane.halignment="CENTER"/>
							</children>
						</GridPane>
					</children>
				</GridPane>
