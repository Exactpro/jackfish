												<Button fx:id="btnStartDefaultApplication" mnemonicParsing="false" onAction="#startDefaultApplication"/>
												<Button fx:id="btnConnectDefaultApplication" mnemonicParsing="false" onAction="#connectDefaultApplication"/>
												<Button fx:id="btnStopDefaultApplication" mnemonicParsing="false" onAction="#stopDefaultApplication"/>
											</children>
											<padding>
												<Insets left="8.0"/>
											</padding>
										</HBox>
									</right>
