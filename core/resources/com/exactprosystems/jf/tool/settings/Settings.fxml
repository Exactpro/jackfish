<?import javafx.scene.control.*?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<GridPane prefHeight="630.0" prefWidth="570.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exactprosystems.jf.tool.settings.SettingsPanelController">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
	</columnConstraints>
	<rowConstraints>
		<RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
		<RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
		<RowConstraints minHeight="30.0" maxHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
	</rowConstraints>
	<children>
		<TabPane fx:id="tabPane">
			<tabs>
				<Tab closable="false" fx:id="mainTab" text="Main"/>
				<Tab closable="false" fx:id="shortCutsTab" text="ShortCuts"/>
				<Tab closable="false" fx:id="logTab" text="Log"/>
				<Tab closable="false" fx:id="colorsTab" text="Colors"/>
				<Tab closable="false" fx:id="gitTab" text="Git"/>
				<Tab closable="false" fx:id="matrixTab" text="Matrix"/>
				<Tab closable="false" fx:id="wizardTab" text="Wizard"/>
			</tabs>
		</TabPane>
		<Separator GridPane.rowIndex="1" GridPane.columnSpan="2"/>
		<Button text="Restore all to default" onAction="#restoreAllToDefault" GridPane.rowIndex="2" GridPane.halignment="LEFT" GridPane.valignment="CENTER"/>
	</children>
</GridPane>
