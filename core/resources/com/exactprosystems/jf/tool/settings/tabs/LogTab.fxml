<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exactprosystems.jf.tool.settings.tabs.LogTabController">
	<children>
		<GridPane hgap="8.0" layoutX="-4.0" layoutY="-1.0" vgap="4.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<columnConstraints>
				<ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label >
					<text><R fx:constant="LOG_TAB_FATAL"/></text>>
				</Label>
				<ColorPicker fx:id="cpFatal" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" />
				<Label GridPane.rowIndex="1" >
					<text><R fx:constant="LOG_TAB_ERROR"/></text>>
				</Label>
				<ColorPicker fx:id="cpError" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
				<Label  GridPane.rowIndex="2">
					<text><R fx:constant="LOG_TAB_WARN"/></text>>
				</Label>
				<ColorPicker fx:id="cpWarn" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="2" />
				<Label GridPane.rowIndex="3" >
					<text><R fx:constant="LOG_TAB_ALL"/></text>>
				</Label>
				<ColorPicker fx:id="cpAll" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="3" />
				<Label GridPane.columnIndex="2" >
					<text><R fx:constant="LOG_TAB_INFO"/></text>>
				</Label>
				<ColorPicker fx:id="cpInfo" maxWidth="1.7976931348623157E308" GridPane.columnIndex="3" />
				<Label GridPane.columnIndex="2" GridPane.rowIndex="1" >
					<text><R fx:constant="LOG_TAB_DEBUG"/></text>>
				</Label>
				<ColorPicker fx:id="cpDebug" maxWidth="1.7976931348623157E308" GridPane.columnIndex="3" GridPane.rowIndex="1" />
				<Label GridPane.columnIndex="2" GridPane.rowIndex="2">
					<text><R fx:constant="LOG_TAB_TRACE"/></text>>
				</Label>
				<ColorPicker fx:id="cpTrace" maxWidth="1.7976931348623157E308" GridPane.columnIndex="3" GridPane.rowIndex="2" />
				<Button onAction="#restoreDefaults" GridPane.columnIndex="3" GridPane.rowIndex="3" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
					<text><R fx:constant="LOG_TAB_RESTORE_DEFAULT"/></text>>
				</Button>
			</children>
		</GridPane>
	</children>
	<padding>
		<Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
	</padding>
</AnchorPane>
