<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<?import java.lang.Double?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exactprosystems.jf.tool.documents.guidic.DictionaryFxController">
	<center>
		<SplitPane fx:id="splitPane" dividerPositions="0.85" orientation="VERTICAL" >
			<items>
				<ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="900" maxWidth="1.7976931348623157E308" prefHeight="700.0" prefWidth="1457.0">
					<content>
						<GridPane fx:id="mainGridPane"  alignment="CENTER" >
							<fx:define>
								<Double fx:id="widthNavigation" fx:value="767.0"/>
								<Double fx:id="widthInfo" fx:value="600.0"/>
								<Double fx:id="height" fx:value="400.0"/>
							</fx:define>
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES" minWidth="$widthNavigation" prefWidth="$widthNavigation" maxWidth="$widthNavigation"/>
								<ColumnConstraints hgrow="SOMETIMES" minWidth="$widthInfo" prefWidth="$widthInfo"/>
							</columnConstraints>
							<rowConstraints>
								<RowConstraints minHeight="$height" maxHeight="$height" prefHeight="$height" vgrow="SOMETIMES"/>
								<RowConstraints minHeight="$height" prefHeight="$height" vgrow="SOMETIMES"/>
							</rowConstraints>
							<children>
							</children>
						</GridPane>
					</content>
				</ScrollPane>
			</items>
		</SplitPane>
	</center>
</BorderPane>


