<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<?import com.exactprosystems.jf.tool.custom.controls.field.CustomFieldWithButton?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.exactprosystems.jf.tool.dictionary.actions.ActionsController">
	<center>
		<GridPane fx:id="mainGrid">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="66.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<GridPane fx:id="elementActionsGrid">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<ComboBox fx:id="comboBoxApps" maxWidth="1.7976931348623157E308" GridPane.columnSpan="3" />
						<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0" GridPane.columnSpan="3" GridPane.rowIndex="1">
							<children>
								<Button fx:id="btnStartApplication" onAction="#startApplication" mnemonicParsing="false" text="Start" />
								<Button fx:id="btnConnectApplication" onAction="#connectApplication" mnemonicParsing="false" text="Connect" />
								<Button fx:id="btnStop" onAction="#stopConnection" mnemonicParsing="false" text="Stop" />
							</children>
						</HBox>
						<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Send keys" onAction="#sendKeysAction" GridPane.rowIndex="2" />
						<CustomFieldWithButton fx:id="tfSendKeys" GridPane.columnIndex="1" GridPane.columnSpan="2" onAction="#sendKeysAction" GridPane.rowIndex="2" />
						<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Click" onAction="#clickAction" GridPane.rowIndex="3" />
						<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Find" onAction="#findAction" GridPane.columnIndex="1" GridPane.rowIndex="3" />
						<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Refresh" onAction="#refresh" GridPane.columnIndex="2" GridPane.rowIndex="3" />
						<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Do it" onAction="#doIt" GridPane.rowIndex="4" />
						<Label text="Switch to" GridPane.halignment="CENTER" GridPane.rowIndex="5" />
						<ComboBox fx:id="comboBoxWindows" onAction="#changeWindow" maxWidth="1.7976931348623157E308" prefWidth="150.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="5" />
						<GridPane GridPane.columnSpan="3" GridPane.rowIndex="6">
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
								<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
							</columnConstraints>
							<rowConstraints>
								<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
							</rowConstraints>
							<children>
								<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Switch to current" onAction="#switchToCurrent"/>
								<Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Switch to parent" onAction="#switchToParent" GridPane.columnIndex="1" />
							</children>
						</GridPane>
					</children>
					<GridPane.margin>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
					</GridPane.margin>
				</GridPane>
				<Label fx:id="imageArea" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1">
					<GridPane.margin>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
					</GridPane.margin>
				</Label>
			</children>
		</GridPane>
	</center>
</BorderPane>
