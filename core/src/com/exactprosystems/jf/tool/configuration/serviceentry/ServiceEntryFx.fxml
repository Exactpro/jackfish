<TitledPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="348.0" prefWidth="999.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exactprosystems.jf.tool.configuration.serviceentry.ServiceEntryFxController">
   <content>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" percentWidth="15.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" percentHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Description" />
            <TextField fx:id="tfServiceDescription" GridPane.columnIndex="1" />
            <Button mnemonicParsing="false" onAction="#removeEntry" text="Remove entry" GridPane.columnIndex="2" />
            <Label text="Service jar" GridPane.rowIndex="1" />
            <BorderPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <center>
                  <TextField fx:id="tfServiceJar" BorderPane.alignment="CENTER" />
               </center>
               <right>
                  <Button mnemonicParsing="false" onAction="#chooseServiceJar" text="..." BorderPane.alignment="CENTER" />
               </right>
            </BorderPane>
            <GridPane fx:id="tableGrid" GridPane.columnSpan="3" GridPane.rowIndex="3">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="33.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="33.0" prefWidth="100.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
               </children>
            </GridPane>
            <HBox alignment="CENTER" spacing="13.0" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
               <children>
                  <Button fx:id="btnStartService" mnemonicParsing="false" onAction="#startService" text="Start" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <Button fx:id="btnStopService" mnemonicParsing="false" onAction="#stopService" text="Stop" />
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#addAll" text="Add all" GridPane.columnIndex="2" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </content>
</TitledPane>
