        #Id;#Action;#Assert;$Size;$Dialog;$AppConnection
        APPPAR3;DialogGetProperties;This.Result == Result.Passed && This.Out.Size != null;;'Dialog';appCon

        #Id;#Action;#Assert;$Position;$Dialog;$AppConnection
