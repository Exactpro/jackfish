            actual;Table
            @;newH;NewColumn
            0;1;4
            1;2;5
            2;3;6
            #EndRawTable

            #Id;#RawTable
            expected;Table
            @;newH;NewColumn
            0;2;5
            1;1;4
            2;3;6
            #EndRawTable

            #Id;#Action;#IgnoreRowsOrder;#Expected;#Actual
            TBLCMP2;TableCompareTwo;true;expected;actual

        #EndStep

    #EndStep

    #Step;#Kind;#For;#Depends
    'Negative testing';;;
        #Step;#Kind;#For;#Depends
