#Id;#TestCase;#Kind;#Depends;#For
TABLE_ADD_COLUMNS;Test case;Never;;
    #Id;#Action;#id;#name
    TBL1;TableCreate;;

    #Action;#Table;#Columns
    TableAddColumns;TBL1.Out;'surname'

    #Assert;#Message
    TBL1.Out.getHeader(2).equals('surname');'column was not added'

    #Action;#Table;#Columns
    TableAddColumns;TBL1.Out;new String[]{'adress','postindex'}

    #Assert;#Message
    TBL1.Out.getHeader(3).equals('adress') && TBL1.Out.getHeader(4).equals('postindex');'column was not added'

    #Action;#Table;#Columns;#Index
    TableAddColumns;TBL1.Out;'age';2

    #Assert;#Message
    TBL1.Out.getHeader(2).equals('age');'column was not added'

    #Action;#Assert;#Table;#Columns;#Index
    TableAddColumns;This.Result == Result.Failed && This.Kind == ErrorKind.WRONG_PARAMETERS;TBL1.Out;'preid';-1

    #Action;#Assert;#Table;#Columns;#Index
    TableAddColumns;This.Result == Result.Failed && This.Kind == ErrorKind.WRONG_PARAMETERS;TBL1.Out;'nnn';999


