    #EndSubCase

    #Id;#SubCase
    onStepFinish;OnStepFinish
        #Id;#Global;#Let
        res;1;res + 'SF' + s + ' '

    #EndSubCase

    #Id;#SubCase;#errr
    onStepError;OnStepError;
        #Id;#Global;#Let
        res;1;res + 'SE' + s + errr.Message + ' '

    #EndSubCase


