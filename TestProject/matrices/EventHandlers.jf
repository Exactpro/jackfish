        s;1;1

    #EndStep

    #Step
    'S2'
        #Id;#Global;#Let
        s;1;2

        #Fail
        's2'

        #OnError
            #Id;#Global;#Let
            res;1;res + 'OnError'


    #EndStep

    #SetHandler;#Kind
    ;OnStepError

